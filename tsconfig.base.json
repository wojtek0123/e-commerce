{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@e-commerce/client-web-app/browse/data-access": [
        "libs/client-web-app/browse/data-access/src/index.ts"
      ],
      "@e-commerce/client-web-app/browse/feature": [
        "libs/client-web-app/browse/feature/src/index.ts"
      ],
      "@e-commerce/client-web-app/browse/ui": [
        "libs/client-web-app/browse/ui/src/index.ts"
      ],
      "@e-commerce/client-web-app/browse/util": [
        "libs/client-web-app/browse/util/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
