{
  "name": "client-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/client-web/src",
  "tags": [
    "scope:client-web",
    "type:app"
  ],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/client-web",
        "index": "apps/client-web/src/index.html",
        "browser": "apps/client-web/src/main.ts",
        "tsConfig": "apps/client-web/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/client-web/src/assets",
          {
            "glob": "**/*",
            "input": "apps/client-web/public"
          }
        ],
        "styles": [
          "node_modules/primeng/resources/primeng.min.css",
          "apps/client-web/src/styles.scss",
          {
            "input": "apps/client-web/src/assets/themes/light.scss",
            "bundleName": "light",
            "inject": false
          },
          {
            "input": "apps/client-web/src/assets/themes/dark.scss",
            "bundleName": "dark",
            "inject": false
          }
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "serviceWorker": "apps/client-web/ngsw-config.json"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "client-web:build:production"
        },
        "development": {
          "buildTarget": "client-web:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "apps/client-web/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "client-web:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/client-web/jest.config.ts"
      }
    }
  }
}